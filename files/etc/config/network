
config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'

config interface 'lan'
	option device 'br0'
	option proto 'static'
	option ipaddr '192.168.4.1'
	option netmask '255.255.255.0'

config interface 'wwan1'
	option proto 'modemmanager'
	option device '/sys/devices/platform/soc/11200000.usb/usb1/1-1/1-1.1'
	option apn 'internet'
	option auth 'none'
	option iptype 'ipv4v6'
	option loglevel 'ERR'
	list allowedauth 'none'
	option metric '6'

config interface 'wwan2'
	option proto 'modemmanager'
	option device '/sys/devices/platform/soc/11200000.usb/usb1/1-1/1-1.2'
	option apn 'internet'
	option auth 'none'
	option iptype 'ipv4v6'
	option loglevel 'ERR'
	list allowedauth 'none'
	option metric '7'

config interface 'wwan3'
	option proto 'modemmanager'
	option device '/sys/devices/platform/soc/11200000.usb/usb1/1-1/1-1.3'
	option apn 'internet'
	option auth 'none'
	option iptype 'ipv4v6'
	option loglevel 'ERR'
	list allowedauth 'none'
	option metric '8'

config interface 'wan1'
	option proto 'dhcp'
	option device 'wan'
	option peerdns '0'
	option metric '1'

config interface 'wan2'
	option proto 'dhcp'
	option device 'lan1'
	option peerdns '0'
	option metric '2'

config interface 'wan3'
	option proto 'dhcp'
	option device 'lan2'
	option peerdns '0'
	option metric '3'

config interface 'wan4'
	option proto 'dhcp'
	option device 'eth1'
	option peerdns '0'
	option metric '4'

config interface 'wan5'
	option proto 'dhcp'
	option device 'eth2'
	option peerdns '0'
	option metric '5'

config device
	option type 'bridge'
	option name 'br0'
	list ports 'lan3'

config rule
	option priority '0'
	option lookup '100'
	option mark '1'

config route
	option interface 'loopback'
	option type 'local'
	option target '0.0.0.0/0'
	option table '100'
